<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <AssemblyName>SpaceEngineers.Core.Extensions</AssemblyName>
    <RootNamespace>SpaceEngineers.Core.Extensions</RootNamespace>
    <Company>SpaceEngineers</Company>
    <Authors>SpaceEngineers</Authors>
    <RepositoryUrl>https://github.com/warning-explosive/Core</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <RunAnalyzersDuringBuild>true</RunAnalyzersDuringBuild>
    <RunAnalyzersDuringLiveAnalysis>true</RunAnalyzersDuringLiveAnalysis>
    <RunAnalyzers>true</RunAnalyzers>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <IsPackable>true</IsPackable>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Nullable>enable</Nullable>
    <TieredCompilation>true</TieredCompilation>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Compile Update="ITypeInfoStorage.cs">
      <DependentUpon>TypeExtensions.cs</DependentUpon>
    </Compile>
    <Compile Update="TypeExtensionsImpl.cs">
      <DependentUpon>ITypeExtensions.cs</DependentUpon>
    </Compile>
    <Compile Update="ITypeExtensions.cs">
      <DependentUpon>TypeExtensions.cs</DependentUpon>
    </Compile>
    <Compile Update="TypeInfoStorage.cs">
      <DependentUpon>ITypeInfoStorage.cs</DependentUpon>
    </Compile>
    <Compile Update="ReferenceEqualityComparer.cs">
      <DependentUpon>ObjectExtensions.DeepCopy.cs</DependentUpon>
    </Compile>
  </ItemGroup>
</Project>