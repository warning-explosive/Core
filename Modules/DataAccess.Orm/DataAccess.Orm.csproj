<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
        <AssemblyName>SpaceEngineers.Core.DataAccess.Orm</AssemblyName>
        <RootNamespace>SpaceEngineers.Core.DataAccess.Orm</RootNamespace>
        <IsPackable>true</IsPackable>
        <Title>SpaceEngineers.Core.DataAccess.Orm</Title>
        <Authors>SpaceEngineers</Authors>
        <Company>SpaceEngineers</Company>
        <PackageDescription>SpaceEngineers.Core.DataAccess.Orm</PackageDescription>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/warning-explosive/Core</RepositoryUrl>
        <Copyright>Copyright (c) 2019</Copyright>
        <CodeAnalysisRuleSet>..\..\ruleset.ruleset</CodeAnalysisRuleSet>
        <RunAnalyzersDuringBuild>true</RunAnalyzersDuringBuild>
        <RunAnalyzersDuringLiveAnalysis>true</RunAnalyzersDuringLiveAnalysis>
        <RunAnalyzers>true</RunAnalyzers>
        <CopyLocalLockFileAssemblies>false</CopyLocalLockFileAssemblies>
        <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
        <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
        <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <TieredCompilation>true</TieredCompilation>
        <Library />
    </PropertyGroup>
    <ItemGroup>
      <ProjectReference Include="..\AutoRegistration\AutoRegistration.csproj" />
      <ProjectReference Include="..\CrossCuttingConcerns.Api\CrossCuttingConcerns.Api.csproj" />
      <ProjectReference Include="..\DataAccess.Contract\DataAccess.Contract.csproj" />
    </ItemGroup>
    <ItemGroup>
      <PackageReference Include="Dapper" Version="2.0.90" />
    </ItemGroup>
    <ItemGroup>
      <Compile Update="Model\Abstractions\CreateDatabase.cs">
        <DependentUpon>IDatabaseModelChange.cs</DependentUpon>
      </Compile>
      <Compile Update="Model\Abstractions\DropTable.cs">
        <DependentUpon>IDatabaseModelChange.cs</DependentUpon>
      </Compile>
      <Compile Update="Model\Abstractions\DropColumn.cs">
        <DependentUpon>IDatabaseModelChange.cs</DependentUpon>
      </Compile>
      <Compile Update="Model\Abstractions\CreateTable.cs">
        <DependentUpon>IDatabaseModelChange.cs</DependentUpon>
      </Compile>
      <Compile Update="Model\Abstractions\ColumnNode.cs">
        <DependentUpon>DatabaseNode.cs</DependentUpon>
      </Compile>
      <Compile Update="Model\Abstractions\AlterColumn.cs">
        <DependentUpon>IDatabaseModelChange.cs</DependentUpon>
      </Compile>
      <Compile Update="Model\Abstractions\AddColumn.cs">
        <DependentUpon>IDatabaseModelChange.cs</DependentUpon>
      </Compile>
      <Compile Update="Model\Abstractions\TableNode.cs">
        <DependentUpon>DatabaseNode.cs</DependentUpon>
      </Compile>
      <Compile Update="Model\Abstractions\ICodeModelBuilder.cs">
        <DependentUpon>IModelBuilder.cs</DependentUpon>
      </Compile>
      <Compile Update="Model\Abstractions\IDatabaseModelBuilder.cs">
        <DependentUpon>IModelBuilder.cs</DependentUpon>
      </Compile>
      <Compile Update="Model\Abstractions\UpsertView.cs">
        <DependentUpon>IDatabaseModelChange.cs</DependentUpon>
      </Compile>
      <Compile Update="Model\Abstractions\DropView.cs">
        <DependentUpon>IDatabaseModelChange.cs</DependentUpon>
      </Compile>
      <Compile Update="Model\Abstractions\ViewNode.cs">
        <DependentUpon>DatabaseNode.cs</DependentUpon>
      </Compile>
    </ItemGroup>
</Project>
