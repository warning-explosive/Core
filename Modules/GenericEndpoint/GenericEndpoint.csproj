<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <AssemblyName>SpaceEngineers.Core.GenericEndpoint</AssemblyName>
    <RootNamespace>SpaceEngineers.Core.GenericEndpoint</RootNamespace>
    <IsPackable>true</IsPackable>
    <Title>SpaceEngineers.Core.GenericEndpoint</Title>
    <Authors>SpaceEngineers</Authors>
    <Company>SpaceEngineers</Company>
    <PackageDescription>SpaceEngineers.Core.GenericEndpoint</PackageDescription>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/warning-explosive/Core</RepositoryUrl>
    <Copyright>Copyright (c) 2019</Copyright>
    <CodeAnalysisRuleSet>..\..\ruleset.ruleset</CodeAnalysisRuleSet>
    <RunAnalyzersDuringBuild>true</RunAnalyzersDuringBuild>
    <RunAnalyzersDuringLiveAnalysis>true</RunAnalyzersDuringLiveAnalysis>
    <RunAnalyzers>true</RunAnalyzers>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <TieredCompilation>true</TieredCompilation>
    <DebugType>full</DebugType>
    <Library />
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.NetAnalyzers" Version="6.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="StyleCop.Analyzers" Version="1.1.118">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Pipeline\UnitOfWorkPipeline.cs">
      <DependentUpon>IMessagePipelineStep.cs</DependentUpon>
    </Compile>
    <Compile Update="Pipeline\QueryReplyValidationPipeline.cs">
      <DependentUpon>IMessagePipelineStep.cs</DependentUpon>
    </Compile>
    <Compile Update="Pipeline\MessageHandlerPipeline.cs">
      <DependentUpon>IMessagePipelineStep.cs</DependentUpon>
    </Compile>
    <Compile Update="Pipeline\ErrorHandlingPipeline.cs">
      <DependentUpon>IMessagePipelineStep.cs</DependentUpon>
    </Compile>
    <Compile Update="UnitOfWork\IntegrationUnitOfWork.cs">
      <DependentUpon>IIntegrationUnitOfWork.cs</DependentUpon>
    </Compile>
    <Compile Update="Pipeline\DefaultRetryPolicy.cs">
      <DependentUpon>IRetryPolicy.cs</DependentUpon>
    </Compile>
    <Compile Update="Pipeline\IRetryPolicy.cs">
      <DependentUpon>ErrorHandlingPipeline.cs</DependentUpon>
    </Compile>
    <Compile Update="Pipeline\AdvancedIntegrationContext.cs">
      <DependentUpon>IAdvancedIntegrationContext.cs</DependentUpon>
    </Compile>
    <Compile Update="Pipeline\IMessagePipelineStep.cs">
      <DependentUpon>IMessagePipeline.cs</DependentUpon>
    </Compile>
    <Compile Update="Endpoint\IExecutableEndpoint.cs">
      <DependentUpon>IRunnableEndpoint.cs</DependentUpon>
    </Compile>
    <Compile Update="Endpoint\GenericEndpoint.cs">
      <DependentUpon>IExecutableEndpoint.cs</DependentUpon>
    </Compile>
    <Compile Update="Endpoint\MessageHandlerExecutor.cs">
      <DependentUpon>IMessageHandlerExecutor.cs</DependentUpon>
    </Compile>
    <Compile Update="UnitOfWork\OutboxStorage.cs">
      <DependentUpon>IOutboxStorage.cs</DependentUpon>
    </Compile>
    <Compile Update="UnitOfWork\OutboxDelivery.cs">
      <DependentUpon>IOutboxMessagesDelivery.cs</DependentUpon>
    </Compile>
    <Compile Update="Pipeline\HandledByEndpointPipeline.cs">
      <DependentUpon>IMessagePipelineStep.cs</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\DependencyInjection\CompositionRoot.Api\CompositionRoot.Api.csproj" />
    <ProjectReference Include="..\GenericEndpoint.Api\GenericEndpoint.Api.csproj" />
    <ProjectReference Include="..\GenericEndpoint.Messaging\GenericEndpoint.Messaging.csproj" />
    <ProjectReference Include="..\IntegrationTransport.Api\IntegrationTransport.Api.csproj" />
  </ItemGroup>
</Project>