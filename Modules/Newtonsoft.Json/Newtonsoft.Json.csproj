<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <AssemblyName>SpaceEngineers.Core.NewtonSoft.Json</AssemblyName>
    <RootNamespace>SpaceEngineers.Core.NewtonSoft.Json</RootNamespace>
    <IsPackable>true</IsPackable>
    <Title>SpaceEngineers.Core.Newtonsoft.Json</Title>
    <Authors>SpaceEngineers</Authors>
    <Company>SpaceEngineers</Company>
    <PackageDescription>SpaceEngineers.Core.Newtonsoft.Json</PackageDescription>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/warning-explosive/Core</RepositoryUrl>
    <Copyright>Copyright (c) 2019</Copyright>
    <CodeAnalysisRuleSet>..\..\ruleset.ruleset</CodeAnalysisRuleSet>
    <RunAnalyzersDuringBuild>true</RunAnalyzersDuringBuild>
    <RunAnalyzersDuringLiveAnalysis>true</RunAnalyzersDuringLiveAnalysis>
    <RunAnalyzers>true</RunAnalyzers>
    <CopyLocalLockFileAssemblies>false</CopyLocalLockFileAssemblies>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <TieredCompilation>true</TieredCompilation>
    <Library />
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Basics\Basics.csproj" />
    <ProjectReference Include="..\AutoWiringApi\AutoWiringApi.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="ObjectTree\DefaultObjectTreeValueReader.cs">
      <DependentUpon>IObjectTreeValueReader.cs</DependentUpon>
    </Compile>
    <Compile Update="ObjectTree\ValueNode.cs">
      <DependentUpon>NodeBase.cs</DependentUpon>
    </Compile>
    <Compile Update="ObjectTree\RootNode.cs">
      <DependentUpon>NodeBase.cs</DependentUpon>
    </Compile>
    <Compile Update="ObjectTree\ArrayNode.cs">
      <DependentUpon>NodeBase.cs</DependentUpon>
    </Compile>
    <Compile Update="ObjectTree\ObjectNode.cs">
      <DependentUpon>NodeBase.cs</DependentUpon>
    </Compile>
    <Compile Update="ObjectTree\NodeBase.cs">
      <DependentUpon>IObjectTreeNode.cs</DependentUpon>
    </Compile>
  </ItemGroup>
</Project>